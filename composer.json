{
    "name": "securas-sas/laravel-cybershield",
    "description": "Web Application Firewall for Laravel using CyberShield API",
    "license": "MIT",
    "type": "package",
    "homepage": "https://github.com/Securas-sas",
    "authors": [
        {
            "name": "Securas SaS",
            "homepage": "https://github.com/Securas-sas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Securas\\LaravelCyberShield\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Securas\\LaravelCyberShield\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Securas\\LaravelCyberShield\\CyberShieldServiceProvider"
            ]
        }
    }
}
